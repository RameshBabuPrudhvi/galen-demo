@objects
    noOfAssets        xpath //div[contains(normalize-space(),'Total Number of Assets')][@class='card card-stats']
    avgPeRatio          xpath //div[contains(normalize-space(),'Average PE Ratio')][@class='card card-stats']
    avgDividendYield      xpath //div[contains(normalize-space(),'Average Dividend Yield')][@class='card card-stats']

= Dashboard Position =
    avgPeRatio:
        right-of noOfAssets 30px

    avgDividendYield:
        right-of avgPeRatio 30px
